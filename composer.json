{
    "name": "appyfurious/admin-user-bundle",
    "type": "symfony-bundle",
    "description": "git@bitbucket.org:appyfurious_team/bundle_admin_user.git",
    "homepage": "https://bitbucket.org/appyfurious_team/bundle_admin_user",
    "license": "proprietary",
    "authors": [
        {
            "name": "Maria Deryusheva",
            "email": "m.deryusheva@appyfurious.com"
        }
    ],
    "repositories": [{
        "type": "composer",
        "url": "https://packagist.appycnt.com"
    }],
    "require": {
        "php": ">=7.4.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ramsey/uuid": "^4.1",
        "symfony/flex": "^1.3.1",
        "symfony/swiftmailer-bundle": "^3.4"
    },
    "require-dev": {
        "doctrine/orm": "^2.7",
        "symfony/symfony": "^5.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "Appyfurious\\AdminUserBundle\\": "" }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.1"
        }
    },
    "scripts": {
        "auto-scripts": {

        }
    }
}
